{
  "name": "diabot",
  "version": "0.9.0",
  "private": true,
  "engines": {
    "node": ">=20.11 <21",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:ci": "NEXT_TELEMETRY_DISABLED=1 NEXT_CACHE_DISABLED=1 next build",
    "build:clean": "npm run clean && npm run reinstall && npm run build:ci",
    "check:unicode-confusables": "node scripts/check_confusables.js || true",
    "prepare": "npx husky install",
    "clean": "rimraf .next node_modules .turbo dist pnpm-lock.yaml package-lock.json yarn.lock",
    "reinstall": "npm ci || npm i --force",
    "smoke": "node ./scripts/smoke.mjs",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:vitest": "vitest run -c vitest.e2e.config.ts",
    "etl:daily": "tsx scripts/etlDaily.ts",
    "etl:weekly": "tsx scripts/etlWeekly.ts",
    "backfill:meal:cache": "node scripts/etl/backfill_meal_cache.mjs",
    "auth:check": "tsx scripts/auth_smoke.ts",
    "meal:check": "tsx scripts/meal_smoke.ts",
    "checkpoint": "npm run typecheck && npm run lint && npm run test && npm run build",
    "stability:check": "node scripts/stability-check.js",
    "stability:fix": "rimraf .next && npm run build:ci && npm run stability:check",
    "start:prod": "next start -p 3001",
    "health": "node -e \"const p=process.env.PORT||3001; fetch('http://localhost:'+p+'/api/health').then(r=>r.text()).then(t=>console.log(t)).catch(e=>{console.error(e.message);process.exit(1)})\"",
    "docker:build": "docker build -t diabot/app:latest .",
    "docker:up": "docker compose up -d --build",
    "docker:logs": "docker compose logs -f diabot",
    "docker:down": "docker compose down",
    "env:sync": "cp .env .env.local",
    "db:seed:demo": "tsx scripts/db_seed_demo.ts",
    "dev-local": "node node_modules/next/dist/bin/next dev",
    "start-local": "node node_modules/next/dist/bin/next start"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "clsx": "^2.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "lucide-react": "0.543.0",
    "next": "14.2.7",
    "openai": "^5.22.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "^3.2.1",
    "tailwind-merge": "^2.2.0",
    "uuid": "^9.0.0",
    "zod": "^3.25.8",
    "zod4": "npm:zod@^4.1.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@playwright/test": "^1.40.0",
    "@swc/core": "^1.13.5",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.3",
    "husky": "^9.1.7",
    "jsdom": "^23.0.0",
    "kill-port": "^2.0.1",
    "lint-staged": "^16.2.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "5.9.2",
    "postcss": "^8.4.47",
  "autoprefixer": "^10.4.20",
    "vitest": "^1.0.0"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "npm run type-check"
    ]
  },
  "overrides": {
    "next": "14.2.7"
  }
}
